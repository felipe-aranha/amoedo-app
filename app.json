{
  "expo": {
    "name": "Amoedo",
    "slug": "amoedo-arquiteto",
    "privacy": "unlisted",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.25.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon-android.png",
    "splash": {
      "image": "./assets/images/splash/splashhdpi.png",
      "resizeMode": "cover",
      "backgroundColor": "#F1CE00"
    },
    "scheme": "amoedoapp",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/images/icon-ios.png",
      "bundleIdentifier": "br.com.amoedo.amoedoapp",
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#F1CE00",
        "resizeMode": "cover",
        "image": "./assets/images/splash/splashhdpi.png",
        "tabletImage": "./assets/images/splash/splashxxhdpi.png"
      }
    },
    "notification": {
      "iosDisplayInForeground": true
    },
    "android": {
      "package": "br.com.amoedo.amoedoapp",
      "versionCode": 1,
      "splash": {
        "backgroundColor": "#F1CE00",
        "resizeMode": "cover",
        "mdpi": "./assets/images/splash/splashmdpi.png",
        "hdpi": "./assets/images/splash/splashhdpi.png",
        "xhdpi": "./assets/images/splash/splashhdpi.png",
        "xxhdpi": "./assets/images/splash/splashxxhdpi.png",
        "xxxhdpi": "./assets/images/splash/splashxxxhdpi.png"
      }
    }
  }
}
